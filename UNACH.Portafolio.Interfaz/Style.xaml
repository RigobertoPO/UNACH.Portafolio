<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="ColorMenu">#012E40</Color>
    <Color x:Key="ColorBarra">#A68877</Color>
    <Color x:Key="ColorFondo">#F2E2DF</Color>
    <Style TargetType="Button">
        <Setter Property="Button.Background" Value="#1B4E59" />
        <Setter Property="Button.Foreground" Value="White" />
        <Setter Property="Button.FontFamily" Value="Comic Sans MS" />
    </Style>
</ResourceDictionary>